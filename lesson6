import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
      System.out.println("ряд фибоначчи");
      Scanner sc= new Scanner(System.in);
      int n = sc.nextInt();
    int sum = 2;
    int n0 = 1;
    int n1 = 1;
    int next = 0;
    int k = 0;
    System.out.print(n0+" "+n1+" ");
     for(int i = 3; i <=n; i++){
			next = n0+n1;
			System.out.print(next +" ");
			n0=n1;
			n1=next;
			sum = n1+sum;
  }
	System.out.println();
	if (sum%2==0){
			  System.out.printf("Сумма первых %d членов ряда\n", n);
			  System.out.printf("четное число,она равна %d\n", sum);
			}
			else{			  
			  System.out.printf("Сумма первых %d членов ряда\n", n);
              System.out.printf("нечетное число ");
			}
    }
}
import java.util.*;
public class Main {
  
    public static void main(String[] args) {
      System.out.println("Задача:Вывод цикла от 10 до 30 "); 
      System.out.println("(while и  do--while) "); 
System.out.println("1 -- while");
      int n = 10;
      while (n<31) {
        System.out.println(n);
        n++;
      }
      
      System.out.println(" ");
      
      System.out.println("2 do--while");
      int n1 = 10;
      do {
        System.out.println(n1);
        n1++;
      }
      while (n1<31);
      
      System.out.println(" ");
    }    
  }
