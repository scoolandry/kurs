import java.util.*;
public class Main 
    public static void main(String[] args) {
    System.out.println("Вывод цикла от 10 до 30 "); 
    System.out.println("(while и  do--while) "); 
    System.out.println("Задача 9.1");
      int n = 10;
      while (n<31) {
        System.out.println(n);
        n++;
      }
      
      System.out.println(" ");
      
      System.out.println("Задача 9.2");
      int n1 = 10;
      do {
        System.out.println(n1);
        n1++;
      }
      while (n1<31);
      
      System.out.println(" ");
    }    
  }
  
